      <autopick random="false"/>
      <watchdog panic="None"/>
      <repos/>
      <packages/>
      <distroRequires>
        <and>
          <distro_family op="=" value="Fedora22"/>
          <distro_variant op="=" value="Server"/>
          <distro_name op="=" value="Fedora-22"/>
